{
	"compilerOptions": {
	"target": "es5",
	"module": "esnext",
	"strict": true,
	"esModuleInterop": true,
	"moduleResolution": "node",
	"skipLibCheck": true,
	"forceConsistentCasingInFileNames": true,
	"lib": ["dom", "dom.iterable", "esnext"],
	"allowJs": true,
	"allowSyntheticDefaultImports": true,
	"isolatedModules": true,
	"baseUrl": "./src",
	"jsx": "react-jsx"
	},
	"include": ["src"]
}
